{
  "name": "websocket.js",
  "version": "0.1.0",
  "description": "WebSocket browser client with reconnect back-off feature.",
  "repository": {
    "type": "git",
    "url": "git://github.com/gamestdio/websocket.js.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "watchify ./index.js -s WebSocketClient -o dist/websocket.js",
    "start": "npm run dev & nodemon ./examples/server.js",
    "uglify": "uglifyjs dist/websocket.js --compress > dist/websocket.min.js",
    "release": "browserify ./index.js -s WebSocketClient --bare -o dist/websocket.js",
    "prepublish": "npm run release && npm run uglify"
  },
  "browserify": {
    "transform": [
      [ "babelify", { "presets": ["es2015"] }]
    ]
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "express": "^4.13.3",
    "nodemon": "^1.7.1",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1",
    "ws": "^0.8.0"
  },
  "dependencies": {
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "babel-preset-es2015": "^6.1.18",
    "backoff": "^2.4.1"
  }
}
