{
  "name": "websocket-client",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/gamestdio/websocket-client.git"
  },
  "index": "dist/websocket-client.js",
  "scripts": {
    "example": "npm run dev & nodemon ./examples/server.js",
    "dev": "watchify index.js -s WebSocketClient -o dist/websocket-client.js",
    "prepublish": "browserify index.js -s WebSocketClient -o dist/websocket-client.js && uglifyjs dist/websocket-client.js > dist/websocket-client.min.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "express": "^4.13.3",
    "nodemon": "^1.7.1",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1",
    "ws": "^0.8.0"
  },
  "dependencies": {
    "backoff": "^2.4.1"
  }
}
